            <Font size="9.0" />
         </font>
      </TextField>
      <Label layoutX="254.0" layoutY="112.0" text="Monthly Speech">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <Button focusTraversable="false" layoutX="282.0" layoutY="311.0" mnemonicParsing="false" onAction="#Create" text="Publish" />
      <Button focusTraversable="false" layoutX="31.0" layoutY="53.0" mnemonicParsing="false" onAction="#backtohome" text="Return to Home" wrapText="true" />
   </children>
</AnchorPane>
