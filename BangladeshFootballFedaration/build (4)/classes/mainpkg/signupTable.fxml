<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mainpkg.SignupTableController">
   <children>
      <TableView fx:id="tableView" layoutX="14.0" layoutY="100.0" prefHeight="200.0" prefWidth="571.0">
        <columns>
          <TableColumn fx:id="name" editable="false" prefWidth="75.0" text="Name" />
          <TableColumn fx:id="email" editable="false" prefWidth="160.0" text="Email" />
            <TableColumn fx:id="password" editable="false" prefWidth="182.0" text="Password" />
            <TableColumn fx:id="uid" editable="false" prefWidth="153.0" text="User ID" />
        </columns>
      </TableView>
      <Button layoutX="292.0" layoutY="325.0" mnemonicParsing="false" onAction="#loadTable" style="-fx-background-color: #ffe0b2; -fx-background-radius: 10;" text="Load" />
      <Button focusTraversable="false" layoutX="33.0" layoutY="36.0" mnemonicParsing="false" onAction="#backtohome" style="-fx-background-color: #ffe0b2; -fx-background-radius: 10;" text="Return to Home" wrapText="true" />
   </children>
</AnchorPane>
